{
  "name": "sky-pi-screenshot",
  "version": "1.0.0",
  "description": "Takes screenshots of the webapp. Very exciting.",
  "type": "module",
  "main": "dist/screenshot.js",
  "source": "src/screenshot.ts",
  "targets": {
    "main": {
      "includeNodeModules": {}
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "bs": "pnpm run build && pnpm run run",
    "build": "rollup -c",
    "run": "node --es-module-specifier-resolution=node dist/screenshot.js"
  },
  "dependencies": {
    "puppeteer": "^22.12.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^4.19.0"
  },
  "packageManager": "pnpm@9.6.0"
}
